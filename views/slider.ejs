    <title>Slider</title>
    <style>
      
      .carousel-caption{
        top: 20%;
        font-size: 30px;
        color: #fff;
      }
      .carousel-item a{
        color: #fff;
        font-size: large;
      }
      .btn-slider:hover {
        color: white;
        background-color: #242c5e;
        border-radius: 50px;
      }
      .descr:hover{
        color: white;
        background: none;
      }
      h1{
        font-size: 4rem;
        font-weight: bold;
      }
      .btn-slider{
        padding: 15px 35px 15px 35px;
        font-size: 14px;
        text-transform: uppercase;
        margin: 30px 0;
        color: #fff;
        background-color: #ecb101;
        border: 1px solid #ecb101;
        border-radius: 50px;
      }
      
      .elementor{
        position: relative;
        top: 10px;
        color: white;
        display: flex;
        width: 100%;
        height: fit-content;
        background-color: #242c5e;
        font-size: large;
      }
      .elementor-image img{
        width: 80px;
        height: 80px;
        
      }
      .elementor-btn{
        margin-top: 30px;
        color: white;
        background-color: #00ffff00;
        border: 4px solid white;
        border-radius: 50px;
        padding: 15px;
      }
      .elementor-btn:hover{
        background-color: #ecb101;
        padding: 15px;
        border-radius: 50px;
        color: white;
      }
      .elementor-but{
        padding-top: 50px;
      }
      .element{
        width: 25%;
        padding: 12px;
        height: 490px;
        background-color: #145b20;
      }
      @media all and (max-width: 1350px) {
        .elementor{
          flex-direction: column;

        }
        .element{
          padding-bottom: 30px;
          width: 100%;
        }
        
}
       
    </style>
  </head>
